package DMAexample.biomes;

import net.minecraft.init.Blocks;
import net.minecraft.world.biome.BiomeGenBase;

public class TestBiome extends BiomeGenBase {

	public TestBiome(int height) {
		super(height);
	
		// Top layer
		topBlock = Blocks.mycelium;
		//3-5 layers below top layer
		fillerBlock = Blocks.melon_block;
		
		setBiomeName("Test Biome");
		setDisableRain();
		
	}
}
