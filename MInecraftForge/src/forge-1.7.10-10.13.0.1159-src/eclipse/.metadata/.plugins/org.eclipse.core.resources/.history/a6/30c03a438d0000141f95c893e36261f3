public class TestMod {
	
	@Instance(value = "GGenericModID")
	public static TestMod instance;
	
	@SidedProxy(clientSide="com.ditto11.examplemod.client.ClientProxy", serverSide="com.ditto11.examplemod.CommonProxy");
	
}
